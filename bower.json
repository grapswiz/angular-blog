{
  "name": "angular-blog",
  "version": "0.0.0",
  "authors": [
    "grapswiz <ineeza@gmail.com>"
  ],
  "description": "my personal blog service",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "jquery": "2.0.3",
    "angular": "1.2.4",
    "gumby": "2.6.0"
  },
  "devDependencies": {
    "jasmine": "1.3.1",
    "angular-mocks": "1.2.4"
  },
  "exportsOverride": {
    "jquery": {
      "main-js": "**/jquery.js"
    },
    "angular": {
      "main-js": "**/angular.js",
      "main-css": "**/*.css"
    },
    "jasmine": {
      "test-js": "lib/jasmine-core/jasmine*.js",
      "test-css": "lib/jasmine-core/*.css"
    },
    "angular-mocks": {
      "test-js": "**/*.js"
    },
    "gumby": {
      "main-sass": "sass/"
    }
  }
}
